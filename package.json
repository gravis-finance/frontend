{
  "name": "@gravis/frontend",
  "version": "0.1.0",
  "private": true,
  "sideEffects": [
    "./src/i18n.ts"
  ],
  "scripts": {
    "analyze": "node ./scripts/bundle-analyze",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:dev": "dotenv -e .env.development react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "generate:config": "ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/generateConfig",
    "locales": "babel --extensions \".tsx\",\".ts\" --config-file ./babel-locales.config.js ./src"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@crowdin/crowdin-api-client": "^1.10.4",
    "@gravis.finance/sdk": "^1.0.6",
    "@gravis.finance/uikit": "^1.3.67",
    "@reduxjs/toolkit": "^1.5.0",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-router-dom": "^5.1.5",
    "@types/styled-components": "^5.1.7",
    "@types/swiper": "^6.0.0",
    "date-fns": "^2.17.0",
    "gsap": "^3.9.1",
    "i18next": "^15.0.9",
    "i18next-browser-languagedetector": "^3.0.1",
    "i18next-xhr-backend": "^2.0.1",
    "lodash": "^4.17.20",
    "node-fetch": "^2.6.1",
    "react": "^17.0.1",
    "react-confetti": "^6.0.1",
    "react-countup": "^4.3.3",
    "react-dom": "^17.0.1",
    "react-feather": "^2.0.8",
    "react-i18next": "^10.7.0",
    "react-intersection-observer": "^8.33.1",
    "react-query": "^3.34.16",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.1",
    "react-use": "^17.2.1",
    "react-youtube": "^7.13.1",
    "styled-components": "^5.1.1",
    "swiper": "^8.0.7",
    "typescript": "^4.1.5"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.14.5",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@pancakeswap-libs/eslint-config-pancake": "^1.0.1",
    "@types/jest": "^26.0.19",
    "@types/react-redux": "^7.1.16",
    "babel-plugin-parse-translation": "^1.0.2",
    "eslint": "^7.20.0",
    "husky": "4.3.5",
    "prettier": "^2.1.2",
    "sass": "^1.49.9",
    "source-map-explorer": "^2.5.2",
    "ts-node": "^9.1.1",
    "webpack-bundle-analyzer": "^4.4.2"
  }
}
